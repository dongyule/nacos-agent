{
    "name": "hcz/nacos-agent",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "hcz/nacos-sdk-php": "dev-master",
        "workerman/workerman": "^4.0"
    },
    "autoload": {
        "psr-4": {
        },
        "files": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "repositories": {
        "hcz/nacos-sdk-php": {
            "type": "vcs",
            "url": "http://gitlab.ops.haochezhu.club/dongyule/nacos-sdk-php.git"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php -r \\\"file_exists('conf/config.json') || copy('conf/config.json.example', conf1/config.json');\\\""
        ]
    }
}
